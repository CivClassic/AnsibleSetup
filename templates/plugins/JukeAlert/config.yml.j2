snitchConfigs:
  note:
    item:
      ==: org.bukkit.inventory.ItemStack
      type: NOTE_BLOCK
      amount: 1
    id: 1
    name: Snitch
    range: 11
    appender:
      broadcast: {}
      dormantcull:
        lifeTime: 2 weeks
        dormantTime: 2 weeks
      showownerondestroy: {}
  juke:
    item:
      ==: org.bukkit.inventory.ItemStack
      type: JUKEBOX
      amount: 1
    id: 2
    name: Logsnitch
    range: 11
    appender:
      broadcast: {}
      dormantcull:
        lifeTime: 4 weeks
        dormantTime: 4 weeks
      showownerondestroy: {}
      levertoggle: {}
      log: {}


database:
  ==: vg.civcraft.mc.civmodcore.dao.ManagedDatasource
  plugin: JukeAlert
  user: '{{ mysql_user }}'
  password: '{{ mysql_non_root_pass }}'
  host: '{{ mysql_host }}'
  port: 3306
  database: '{{ mysql_db_name }}'
  poolsize: 5
  connection_timeout: 10000
  idle_timeout: 600000
  max_lifetime: 7200000  

